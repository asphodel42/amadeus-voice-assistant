# Copy this file to .env and fill in your values

# Picovoice Access Key (for wake word detection)
# Get your key from: https://console.picovoice.ai/
PICOVOICE_ACCESS_KEY=your_key_here_get_from_console_picovoice_ai

# Path to Vosk model (legacy, not used with Whisper)
VOSK_MODEL_PATH=models/vosk-model

# Data folder for audit database and logs
# Default: data/
DATA_FOLDER=data